fisher_tbl <- tibble::tribble(
  ~letter, ~emoji,
  "a", c("🅰️", "⛺"),
  "b", "🅱️",
  "c", c("🥐", "📞"),
  "d", c("↩️", "👂"),
  "e", c("📧", "3️⃣"),
  "f", c("🥀", "🎏"),
  "g", "🗜",
  "h", c("♓️", "🙌"),
  "i", c("🕯", "ℹ️"),
  "j", c("🌶", "🗾"),
  "k", "🎋",
  "l", c("👍", "🕒", "👢"),
  "m", c("♏️", "♍️", "Ⓜ️", "〽️"),
  "n", "♑️",
  "o", c("🤡", "😋", "🙆", "❄️", "💿", "🅾️", "\U0002b55", "\U0001F518", "\U0001F535", "\U0001F534", "\U00026AB", "\U00026AA", "❤️", "\U0001F300"),
  "p", "🅿️",
  "q", c("🗨", "🍳", "🎅"),
  "r", c("🌱", "\U00AE"),
  "s", c("⚡️", "\U0001F4B2"),
  "t", c("✝️", "\U0001F334", "\U0002795"),
  "u", c("\U00026CE", "\U0001F319", "👅"),
  "v", c("✌️", "\U0001F596", "♈️"),
  "w", c("\U0001F450", "\U0001F335"),
  "x", c("⚒", "\U0001F6E0", "⚔️", "\U0002694", "\U000274C", "✂️"),
  "y", c("\U0001F938", "\U0001F378"),
  "z", c("\U0001F4A4", "\U00024E9"),
  "!", c("❕", "❗️"),
  "?", c("❓", "❔")
)

fisher_lst <- tibble::deframe(fisher_tbl)

save(fisher_lst, file = "fisher_lst.Rda")
